{
  "name": "todo-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "branch": "git branch gh-pages",
    "switch": "git checkout gh-pages",
